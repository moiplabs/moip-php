{
    "name": "raniellyferreira/moip-php",
    "description": "Biblioteca de integração com o MoIP",
    "license": "MIT",
    "homepage": "https://labs.moip.com.br/biblioteca/",
	"type" : "library",
	"archive": {
        "exclude": ["autoload.inc.php"]
    },
    "autoload": {
        "files": [
			"lib/Moip.php",
			"lib/MoipClient.php",
			"lib/MoipStatus.php"
		]
    },
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "lib-curl": "*",
        "lib-openssl": "*",
        "lib-libxml": "*"
    }
}